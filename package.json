{
  "name": "francis-component-react",
  "version": "0.0.5",
  "description": "基于 arco-design 组件库的 react 组件库，用于扩充 arco-design 组件库的功能",
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "umd": {
    "file": "./dist/index.min.js",
    "module": "arcoMaterial",
    "style": "./dist/css/index.css"
  },
  "scripts": {
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "add:component": "node ./scripts/addComponent.js",
    "eslint": "eslint tests/ components/ --fix --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint 'components/**/*.less' --fix --cache",
    "format": "prettier '{tests,components}/**/*.{js,jsx,ts,tsx}' --config .prettierrc --write ",
    "clean": "rm -rf es lib dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@arco-design/web-react": "~2",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@arco-design/web-react": "^2.28.0",
    "@storybook/addon-actions": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.24",
    "@types/node": "^24.0.15",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/shallowequal": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "chalk": "^2.4.2",
    "concurrently": "^5.3.0",
    "cross-spawn": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-typescript": "^0.14.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "husky": "^4.3.6",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "storybook": "^8.3.5",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "typescript-json-schema": "^0.45.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css, less}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "sideEffects": [
    "dist/**/*",
    "{es,lib,components}/**/style/*",
    "*.less"
  ],
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "arcoMeta": {
    "type": "react-library",
    "title": "123",
    "category": []
  },
  "license": "MIT"
}
